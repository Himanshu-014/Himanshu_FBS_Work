#include <stdio.h>

int main() {
    int a, b, c, d, e;
    float average;

    printf("Enter five numbers Respectively to a b c d e:\n");
    scanf("%d %d %d %d %d", &a, &b, &c, &d, &e);

    average = (a + b + c + d + e) / 5.0;

    printf("The average of %d, %d, %d, %d, %d is: %.2f\n", 
           a, b, c, d, e, average);

    return 0;
}
